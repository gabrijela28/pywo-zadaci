    "def fib_gen(limit=20):\n",
    "  \n",
    "    \n",
    "    a,b =0,1\n",
    "    \n",
    "    while a<limit:\n",
    "        yield a\n",
    "        a,b = b, a+b\n",
    "\n",
    "fg = fib_gen(limit=5)\n",
    "\n",
    "for x in fg:\n",
    "    print(x)\n",
    "\n"
   ]
